{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.3",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "auto",
                    "minHeight": "150px",
                    "verticalContentAlignment": "Center",
                    "style": "default",
                    "items": [
                        {
                            "type": "Image",
                            "width": "300px",
                            "height": "150px",
                            "url": "https://dhabadeliciousstorage.blob.core.windows.net/restaurant-images/restaurant-2-veg.jpg",
                            "size": "Medium"
                        },
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "style": "default",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Dhaba Delicious-Tiffins",
                                            "size": "medium",
                                            "wrap": true,
                                            "weight": "Bolder",
                                            "color": "Dark"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "url": "https://dhabadeliciousstorage.blob.core.windows.net/icons/icons8-veg-48.png",
